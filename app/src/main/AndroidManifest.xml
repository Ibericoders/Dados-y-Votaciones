<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sergioescalona.dadosyvotacionesapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dicer_launcher"
        android:label="@string/text_view_dices_votes"
        android:roundIcon="@mipmap/ic_dicer_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Project.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Project.Activities.DicesActivity" />
        <activity android:name=".Project.Activities.VoteActivity" />
        <activity android:name=".Project.Activities.ResultActivity" />
        <activity android:name=".Project.Activities.ConfigVotationActivity"></activity>
    </application>

</manifest>